# ============================================================
# NubHQ Environment Configuration
# Copy to .env.local and fill in values
# ============================================================

# API Configuration
VITE_API_BASE_URL=http://localhost:8000
VITE_API_TIMEOUT=30000

# Feature Flags
VITE_ENABLE_DRY_RUN=true
VITE_ENABLE_VOICE_CHECK=true
VITE_ENABLE_AUTO_SAVE=true
VITE_ENABLE_OFFLINE_MODE=true

# Analytics (optional)
VITE_ANALYTICS_ID=

# Debug
VITE_DEBUG=false
VITE_LOG_LEVEL=warn
